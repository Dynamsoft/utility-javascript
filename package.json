{
  "name": "dynamsoft-utility",
  "version": "1.0.10",
  "description": "The Dynamsoft Utility module provides common auxiliary classes shared across all Dynamsoft SDKs.",
  "homepage": "https://dynamsoft.com/capture-vision/docs/web/programming/javascript/?ver=latest",
  "main": "./dist/utility.js",
  "module": "./dist/utility.esm.js",
  "files": [
    "/dist/*.js",
    "/dist/*.mjs",
    "/dist/*.html",
    "/dist/*.wasm",
    "/dist/**/*.d.ts",
    "/API Reference.url",
    "/LEGAL.txt"
  ],
  "exports": {
    ".": {
      "types": {
        "require": "./dist/types/utility.d.cts",
        "import": "./dist/types/utility.d.ts"
      },
      "import": "./dist/utility.esm.js",
      "require": "./dist/utility.js"
    }
  },
  "types": "./dist/types/utility.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup --config",
    "update:readme": "node ./updateReadme.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dynamsoft/utility-javascript.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": {
    "name": "Dynamsoft",
    "url": "https://www.dynamsoft.com"
  },
  "keywords": [
    "dynamsoft",
    "capture",
    "vision",
    "utility",
    "javascript",
    "sdk"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.4.9",
    "highlight.js": "^9.18.5",
    "rollup": "^2.79.1",
    "showdown": "^1.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dynamsoft-core": "^3.0.10"
  }
}
